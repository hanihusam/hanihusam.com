{
  "extends": "stylelint-config-recommended",
  "rules": {
    "at-rule-no-unknown": [ true, {
      "ignore": ["default-namespace"]
      "ignoreAtRules": [
        "extends",
        "layer"
      ]
    }]
    "property-no-unknown": [ true, {
      "ignoreAtRules": ["/regex/", /regex/, "string"]
    }],
    "block-no-empty": null,
    "unit-whitelist": ["em", "rem", "s", "vw", "vh", "%"]
  }
}
