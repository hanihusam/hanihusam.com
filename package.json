{
  "name": "hanihusam.com",
  "version": "1.2.0",
  "engines": {
    "node": "18"
  },
  "sideEffects": false,
  "description": "A remix version of hanihusam.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hanihusam/hanihusam.com.git"
  },
  "keywords": [],
  "author": "Hani Husamuddin <hi@hanihusam.com> (https://hanihusam.com/)",
  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://github.com/hanihusam/hanihusam.com/issues"
  },
  "homepage": "https://github.com/hanihusam/hanihusam.com#readme",
  "scripts": {
    "prepare": "husky install",
    "build": "run-s build:*",
    "build:remix": "remix build \"node ./other/build-info.js\"",
    "build:server": "esbuild --platform=node --format=cjs ./server/index.ts --outfile=build/server.js",
    "dev": "run-p dev:*",
    "dev:build": "cross-env NODE_ENV=development npm run build:server -- --watch",
    "dev:content": "node -r esbuild-register -r dotenv/config other/refresh-on-content-change.ts",
    "dev:app": "remix dev -c \"node --inspect --require dotenv/config --require ./tests/mocks ./build/server.js\"",
    "format": "prettier --write .",
    "format:repo": "npm run format && npm run lint:repo -- --fix",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:repo": "npm run lint -- --config .eslintrc.repo.js",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "start": "cross-env NODE_ENV=production node ./build/server.js",
    "start:mocks": "cross-env NODE_ENV=production node --require ./mocks --require dotenv/config ./build/server.js",
    "curl:blog": "curl --HEAD --silent http://localhost:$PORT/blog > /dev/null || true",
    "prime-cache:prod": "cross-env PORT=4001 npx start-server-and-test start http-get://localhost:4001/build/info.json curl:blog && echo cache primed ⚡️",
    "prime-cache:mocks": "cross-env PORT=4001 npx start-server-and-test start:mocks http-get://localhost:4001/build/info.json curl:blog && echo cache primed ⚡️",
    "test": "vitest",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"npx cypress open\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"npx cypress run\"",
    "typecheck": "tsc",
    "validate": "./other/validate"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@cld-apis/types": "^0.1.6",
    "@headlessui/react": "^1.7.13",
    "@heroicons/react": "^2.0.16",
    "@octokit/plugin-throttling": "^3.5.2",
    "@octokit/rest": "^18.12.0",
    "@prisma/client": "^5.4.2",
    "@reach/checkbox": "^0.18.0",
    "@remix-run/node": "2.0.1",
    "@remix-run/react": "2.0.1",
    "@remix-run/serve": "2.0.1",
    "@remix-run/server-runtime": "2.0.1",
    "@tailwindcss/typography": "^0.5.9",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^8.7.0",
    "cachified": "^3.5.4",
    "close-with-grace": "^1.2.0",
    "cloudinary-build-url": "^0.2.4",
    "clsx": "^1.2.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "esbuild": "^0.18.2",
    "execa": "^7.1.1",
    "express": "^4.18.2",
    "framer-motion": "^10.16.4",
    "gray-matter": "^4.0.3",
    "isbot": "^3.7.0",
    "litefs-js": "^1.1.2",
    "lru-cache": "^9.1.2",
    "match-sorter": "^6.3.1",
    "mdast-util-mdx-jsx": "^3.0.0",
    "mdx-bundler": "^9.2.1",
    "morgan": "^1.10.0",
    "p-queue": "^7.3.4",
    "pm2": "^5.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reading-time": "^1.5.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-pretty-code": "^0.10.0",
    "rehype-prism-plus": "^1.6.3",
    "rehype-slug": "^5.1.0",
    "remark-gfm": "^3.0.1",
    "shiki": "^0.14.3",
    "source-map-support": "^0.5.21",
    "tailwind-merge": "^1.8.1",
    "tiny-invariant": "^1.3.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@faker-js/faker": "^7.6.0",
    "@remix-run/dev": "2.0.1",
    "@remix-run/eslint-config": "2.0.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/better-sqlite3": "^7.6.4",
    "@types/compression": "^1.7.2",
    "@types/eslint": "^8.4.10",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.1",
    "@types/inquirer": "^9.0.3",
    "@types/lodash.kebabcase": "^4.1.7",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.16.17",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@types/source-map-support": "^0.5.9",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "@vitejs/plugin-react": "^2.2.0",
    "@vitest/coverage-c8": "^0.24.5",
    "autoprefixer": "^10.4.14",
    "binode": "^1.0.5",
    "c8": "^7.12.0",
    "cookie": "^0.5.0",
    "esbuild-register": "^3.4.2",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-remix-react-routes": "^1.0.5",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "fs-extra": "^11.1.1",
    "happy-dom": "^6.0.4",
    "husky": "^7.0.0",
    "inquirer": "^9.2.7",
    "lint-staged": "^13.0.4",
    "lodash.kebabcase": "^4.1.1",
    "msw": "^1.3.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.5",
    "prisma": "^5.4.2",
    "remix-flat-routes": "^0.5.8",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.3.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.3",
    "vite": "^3.2.7",
    "vite-tsconfig-paths": "^3.5.2",
    "vitest": "^0.24.5"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "prettier -w"
    ],
    "*.{json,css,scss,md,mdx}": [
      "prettier -w"
    ]
  }
}
