{
  "name": "hanihusam.com",
  "version": "1.0.0",
  "engines": {
    "node": "18",
    "npm": "8"
  },
  "description": "A remix version of hanihusam.com",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hanihusam/hanihusam.com.git"
  },
  "keywords": [],
  "author": "Hani Husamuddin <me@hanihusam.com> (https://hanihusam.com/)",
  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://github.com/hanihusam/hanihusam.com/issues"
  },
  "homepage": "https://github.com/hanihusam/hanihusam.com#readme",
  "scripts": {
    "prepare": "husky install",
    "build": "remix build",
    "dev": "cross-env NODE_ENV=development binode --require ./mocks -- @remix-run/dev:remix dev",
    "format": "prettier --write .",
    "format:repo": "npm run format && npm run lint:repo -- --fix",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:repo": "npm run lint -- --config .eslintrc.repo.js",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "start": "remix-serve build",
    "start:mocks": "binode --require ./mocks -- @remix-run/serve:remix-serve build",
    "test": "vitest",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"npx cypress open\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"npx cypress run\"",
    "typecheck": "tsc",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@cld-apis/types": "^0.1.6",
    "@headlessui/react": "^1.7.13",
    "@heroicons/react": "^2.0.16",
    "@prisma/client": "^4.15.0",
    "@remix-run/node": "^1.17.0",
    "@remix-run/react": "^1.17.0",
    "@remix-run/serve": "^1.17.0",
    "@remix-run/server-runtime": "^1.17.0",
    "bcryptjs": "^2.4.3",
    "cloudinary-build-url": "^0.2.4",
    "clsx": "^1.2.1",
    "date-fns": "^2.30.0",
    "esbuild": "^0.18.2",
    "gray-matter": "^4.0.3",
    "isbot": "^3.6.3",
    "lru-cache": "^9.1.2",
    "mdx-bundler": "^9.2.1",
    "pm2": "^5.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reading-time": "^1.5.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-prism-plus": "^1.5.1",
    "rehype-slug": "^5.1.0",
    "remark-gfm": "^3.0.1",
    "tailwind-merge": "^1.8.1",
    "tiny-invariant": "^1.3.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@faker-js/faker": "^7.6.0",
    "@remix-run/dev": "^1.14.1",
    "@remix-run/eslint-config": "^1.14.3",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/eslint": "^8.4.10",
    "@types/node": "^18.16.17",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "@vitejs/plugin-react": "^2.2.0",
    "@vitest/coverage-c8": "^0.24.5",
    "autoprefixer": "^10.4.14",
    "binode": "^1.0.5",
    "c8": "^7.12.0",
    "cookie": "^0.5.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.26.0",
    "eslint-config-kdnj": "^2.0.4",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "happy-dom": "^6.0.4",
    "husky": "^7.0.0",
    "lint-staged": "^13.0.4",
    "msw": "^1.2.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.18",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "prisma": "^4.15.0",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.3.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.8.4",
    "vite": "^3.2.7",
    "vite-tsconfig-paths": "^3.5.2",
    "vitest": "^0.24.5"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,yml,yaml}": "eslint --cache --fix",
    "*.{js,jsx,ts,tsx,css,md}": "prettier --write"
  }
}
